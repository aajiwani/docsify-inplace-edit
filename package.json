{
  "name": "docsify-inplace-edit",
  "version": "1.0.0",
  "description": "It will allow you to have edit button within the markdown rendered pages; To edit the portion in place.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx parcel build src/index.js --no-optimize --no-source-maps",
    "park-build-artifact": "rm -rf ./example/dist && mkdir ./example/dist && mv index.js ./example/dist/docsify-inplace-edit.js",
    "ci_tests": "npx cypress run --headless -b chrome",
    "local_tests": "npx cypress open",
    "example": "npm run build && npm run park-build-artifact && docsify serve example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aajiwani/docsify-inplace-edit.git"
  },
  "keywords": [
    "docsify",
    "in-place",
    "editor"
  ],
  "author": "Amir Ali Jiwani <amirali.jiwani89@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aajiwani/docsify-inplace-edit/issues"
  },
  "homepage": "https://github.com/aajiwani/docsify-inplace-edit#readme"
}
